.header-banner(style="background-image: url('img/banner.jpg')")
header.header
    .container.header-row
        .header-info
            .header-info__row
                .header-info__title Время на Кипре:
                .header-info__value
                    span 21:00
            .header-info__row
                .header-info__title О погоде:
                .header-info__value
                    span 22&#176;С
                    img(src="img/rain.png", alt="")
        .header-logo
